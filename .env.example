# Server Configuration
PORT=3000
BASE_URL=https://your-domain.com

# Optional: ElevenLabs for better TTS
ELEVENLABS_API_KEY=your_elevenlabs_key_here
ELEVENLABS_VOICE_ID=your_voice_id_here
